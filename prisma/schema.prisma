generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Champions {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  driver String
  driverCountry String
  copilot String
  copilotCountry String
  car String
  year String
}